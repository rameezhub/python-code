a="18"
b="6"
print("the sum is:",int(a)+int(b))
print("the subtraction is:",int(a)-int(b))
print("the  mul is:",int(a)*int(b))
print("the devision is:",int(a)/int(b))
print("the  modulus is:",int(a)%int(b))
print("the e is:",int(a) //int(b))
